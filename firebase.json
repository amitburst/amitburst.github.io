{
  "hosting": {
    "public": ".",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "redirects": [{
      "source": "/*",
      "destination": "/",
      "type": 301
    }],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
